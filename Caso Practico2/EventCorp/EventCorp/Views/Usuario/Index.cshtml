@model IEnumerable<Usuario>

<table>
    <thead>
        <tr>
            <th>Nombre de Usuario</th>
            <th>Nombre Completo</th>
            <th>Correo</th>
            <th>Telefono</th>
            <th>Rol</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var usuario in Model)
        {
            <tr>
                <td>@usuario.NombreUsuario</td>
                <td>@usuario.NombreCompleto</td>
                <td>@usuario.Correo</td>
                <td>@usuario.Telefono</td>
                <td>@usuario.Rol</td>
            </tr>
        }
    </tbody>
</table>
